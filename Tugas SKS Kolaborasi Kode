# kolaborasi-kode
tugas strategi keamanan siber

daftar_makanan = []

def tampilkan_daftar():
    print("\nDaftar Makanan:")
    if not daftar_makanan:
        print("Daftar makanan kosong.")
    else:
        for i, makanan in enumerate(daftar_makanan, start=1):
            print(f"{i}. {makanan}")
